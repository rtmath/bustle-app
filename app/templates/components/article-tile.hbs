{{yield}}
<div class="article-container">
  <div class="left">
    <img src={{article.image}}/>
  </div>
  <div class="right">
    <div class="right-top">
      <h3>{{article.title}}</h3>
      <span class="body">{{article.body}}</span>
    </div>
    <div class="right-bottom">
      <span class="author">{{article.author}}</span>
      <span class="timestamp">{{article.timestamp}}</span>

    </div>
  </div>
</div>
